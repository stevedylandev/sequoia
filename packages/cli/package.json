{
	"name": "sequoia-cli",
	"version": "0.3.0",
	"type": "module",
	"bin": {
		"sequoia": "dist/index.js"
	},
	"files": [
		"dist",
		"README.md"
	],
	"main": "./dist/index.js",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
